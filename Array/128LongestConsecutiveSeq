class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        num_set=set(nums)
        longest=0
        for num in num_set:
            if num-1 not in num_set:
                current=num
                length=1

                while current+1 in num_set:
                    current+=1
                    length+=1
                longest=max(longest, length)
        return longest


#Brute

def linearSearch(arr, x):
    for num in arr:
        if num==x:
            return True
    return False
def longest(arr):
    longest=1
    for i in range(len(arr)):
        x=arr[i]
        count=1
        while linearSearch(arr, x+1):
            x=x+1
            count=count+1
        longest=max(longest, count) 
    return longest
arr=[100, 4, 200, 1, 3, 2]
print(longest(arr))           

        
